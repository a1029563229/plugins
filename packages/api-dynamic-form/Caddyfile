     methods *
     allow_credentials false
     allowed_headers   X-Custom-Header,X-Foobar,content-type,Access-Control-Allow-Headers,x-requested-with,Access-Control-Request-Headers,Content-type,X-Auth-Token,antikey,authorization,xd-request-id
     exposed_headers   X-Something-Special,SomethingElse
